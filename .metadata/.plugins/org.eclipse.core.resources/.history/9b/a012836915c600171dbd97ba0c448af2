package com.juan.herencia;

import com.juan.herencia.animales.Gallina;
import com.juan.herencia.animales.Gato;

public class Main {

	public static void main(String[] args) {
		
		Gato misino = new Gato (4, false, "misino", 7, null, 800);
		
		Gallina pitas = new Gallina (2, true, "pitas", 1500, 1, null);
		
		misino.comer(pitas);
		

	}

}
