
public class Modelo {

		public int valor;
		public int limite = 10;
		
		public Modelo (int valor){
			this.valor = valor;
		}
		
		public String toString(int valor){
			return "" + valor;
		}
		
		public int[] getTabla(){
			int i = 1;
			int result[] = new int [9];
			
			for (i = 1; i <= this.limite; i++){
				result[i-1] = i*valor;  				
			}
			return result;		
		}
}
