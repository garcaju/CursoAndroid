import java.util.Random;

public class Logic {

	int number;
	int numberEnigma;
	Random numberRandom = new Random();
	int top = 10;

	public Logic (int numberEnigma){
		this.number = number;
		this.numberEnigma = numberRandom.nextInt(10);
	}

	public boolean testGuessNumber(int valor){

		if (valor < this.numberEnigma){
			return  false;
		}else if (valor > this.numberEnigma){
			return false;
		}
		return true;
	}
}
